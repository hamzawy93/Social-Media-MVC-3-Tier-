@using Social_Media.DAL.Entity
@model User
@{
    ViewData["Title"] = "Create";
}

<form asp-action="Create" asp-controller="User">  
    <div class="form-group">
        <label for="exampleInputEmail1">Email address</label>
        <input asp for="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">
        <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Password</label>
        <input asp for="password" class="form-control" id="exampleInputPassword1" placeholder="Password">

        <div class="form-group">
            <label for="exampleInputFullName1">FullName</label>
            <input asp for="FullName" class="form-control" id="exampleInputPassword1" placeholder="FullName">

            <div class="form-group">
                <label for="exampleInputAge1">age</label>
                <input asp for="age" class="form-control" id="exampleInputPassword1" placeholder="age">
    </div>
    <div class="form-check">
        <input type="checkbox" class="form-check-input" id="exampleCheck1">
        <label class="form-check-label" for="exampleCheck1">Check me out</label>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>

